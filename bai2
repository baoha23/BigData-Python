t = int(input('Nhập vào số test:'))
for i in range(1,t+1):
  str = input("nhập chuôi vào: ");
  nguyenam=0
  phuam=0
  str.lower()
  for i in str:
    if(i == 'a'or i == 'e'or i == 'i'or i == 'o'or i == 'u' or i == 'y' ):
      nguyenam=nguyenam+1;
    else:
      phuam=phuam+1;
  print(f"Nguyên âm :{nguyenam}", end="\n")
  print(f"Phụ âm :{phuam}", end="\n")
